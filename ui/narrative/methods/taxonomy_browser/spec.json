{
    "ver": "0.0.1",
    "authors": [
        "janaka"
    ],
    "contact": "https://kbase.us/contact-us/",
    "categories": ["active"],
    "widgets": {
        "input": null,
        "output": "no-display"
    },
    "parameters": [
        {
            "id": "scientific_name",
            "optional": false,
            "advanced": false,
            "allow_multiple": true,
            "default_values": [ "Klebsiella oxytoca" ],
            "field_type": "autocomplete",
            "service" : "taxonomy_service",
            "requires_selection" : true,
            "text_options": {
                "validate_as": "text"
            }
        }
    ],
    "behavior": {
        "service-mapping": {
            "url": "",
            "name": "taxonomy_service",
            "method": "search_taxonomy",
            "input_mapping": [
                {
                    "narrative_system_variable": "workspace",
                    "target_property": "workspace"
                },
                {
                    "input_parameter": "scientific_name",
                    "target_property": "scientific_name"
                }
            ],
            "output_mapping": [
                {
                  "constant_value": "KBaseGenomeAnnotations.Taxon",
                  "target_property": "type"
                },
                {
                  "narrative_system_variable": "workspace",
                  "target_property": "ws"
                }
            ]
        }
    },
    "job_id_output_field": "docker"
}
